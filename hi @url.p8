--explore state

function mk_explore_st()
	local st={}
	
	npcs={
		mk_cloak_npc(20,20)
	}
	
	function st:update()
		for npc in all(npcs) do
			npc:update()
		end
	end
	
	function st:draw()
		for npc in all(self.npcs) do
			npc:draw()
		end
	end
	
	return st
end
