--explore entities

function mk_cloak_npc(x,y)
	local npc={x,y}
	
	local ani_opt={
		af_tbl={{2,3},{3,2}}
	}
	
	npc.anim=mk_animator(ani_opt)
	
	function npc:update()
		self.anim:update()
			
	end
	
	function npc:draw()
		self.anim:draw(self.x,self.y)
	end
	
end
